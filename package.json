{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "description": "A high-performance Next.js 15 boilerplate with Material UI, TypeScript, and internationalization",
  "private": true,
  "author": "Next.js Boilerplate Team",
  "license": "MIT",
  "homepage": "https://github.com/nextjs-boilerplate/nextjs-boilerplate",
  "repository": {
    "type": "git",
